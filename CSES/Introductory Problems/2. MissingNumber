#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    int A[n-1];
    for(int i = 0; i < n-1; i++){
        cin >> A[i];
    }
    sort(A,A+(n-1));
    for(int i=0;i<n-1;++i){
        if(A[i] != i+1){
            cout << i+1 << endl;
            break;
        }
    }
    if(A[n-2] == n-1){
        cout << n << endl;
    }
}
